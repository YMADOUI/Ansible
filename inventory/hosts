[mikrotik]
# Définition des équipements Mikrotik par défaut
mikrotik_master ansible_host=192.168.88.2
mikrotik_slave ansible_host=192.168.88.3

[mikrotik:vars]
ansible_connection=ssh
ansible_user=admin
ansible_ssh_common_args='-o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null'
ansible_port=22
